{
  "manifest_version": 2,
  "name": "Axis Media Control",
  "version": "1.0",
  "background": {
    "scripts": ["knockout.js","ping.js"]
  },
	"content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["jquery.js","knockout.js","content.js","joystick.js"]
	  }
	],
	"browser_action": {
    "default_icon": "img/icon.png",
    "default_popup":"test.html",
     "permissions": ["management"]
  }
}
